{
    "name": "plugins/media-manager",
    "description": "Media Manager plugin for Laravel Filament",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Plugins\\MediaManager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Your Name",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^12.0",
        "filament/filament": "^2.0|^3.0",
        "intervention/image": "^2.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Plugins\\MediaManager\\MediaManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
